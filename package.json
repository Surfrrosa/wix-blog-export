{
  "name": "wix-blog-export",
  "version": "1.0.0",
  "description": "Export your Wix blog posts to Markdown, JSON, and CSV formats",
  "main": "dist/src/cli.js",
  "bin": {
    "wix-blog-export": "./dist/src/cli.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src lib tests --ext .ts",
    "lint:fix": "eslint src lib tests --ext .ts --fix",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "start": "node dist/src/cli.js"
  },
  "keywords": [
    "wix",
    "blog",
    "export",
    "markdown",
    "json",
    "csv",
    "content-migration",
    "cli"
  ],
  "author": "Surfrrosa",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Surfrrosa/wix-blog-export.git"
  },
  "bugs": {
    "url": "https://github.com/Surfrrosa/wix-blog-export/issues"
  },
  "homepage": "https://github.com/Surfrrosa/wix-blog-export#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.2",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "archiver": "^6.0.1",
    "p-limit": "^5.0.0"
  }
}