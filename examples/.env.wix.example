# Wix API Configuration
# Get these values from: https://manage.wix.com/account/api-keys

# Your Wix API key (starts with "IST.")
WIX_API_KEY=IST.your_api_key_here

# Your Wix Account ID (found in dashboard URL or account settings)
WIX_ACCOUNT_ID=your-account-id-here

# Your Site ID (extract from dashboard URL: /dashboard/[SITE-ID]/)
WIX_SITE_ID=your-site-id-here